<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#00936a</Color>
    <Color x:Key="Secondary">#49b899</Color>
    <Color x:Key="Tertiary">#bef6e6</Color>
    <Color x:Key="BackgroundPage">#ffffff</Color>
    <Color x:Key="PrimaryFont">#ffffff</Color>
    <Color x:Key="SecondaryFont">#40806f</Color>
    <Color x:Key="BackgroundPageFont">#00936a</Color>
    <Color x:Key="Alert">#ff0000</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}" />
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}" />
    <SolidColorBrush x:Key="BackgroundPageBrush" Color="{StaticResource BackgroundPage}" />
    <SolidColorBrush x:Key="PrimaryFontBrush" Color="{StaticResource PrimaryFont}" />
    <SolidColorBrush x:Key="BackgroundPageFontBrush" Color="{StaticResource BackgroundPageFont}" />
    <SolidColorBrush x:Key="AlertBrush" Color="{StaticResource Alert}" />

</ResourceDictionary>